<!DOCTYPE html>
<html>
<head>
  <title>SimpleMusicDb</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>

<% if current_user %>
  <%= link_to 'Logout', user_session_path, :method => :delete %>
<% else %>
  <%= link_to 'Login', new_user_session_path %> or <%= link_to 'Sign up', new_user_path %> 
<% end %>

<%= yield %>

</body>
</html>
